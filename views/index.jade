extends layout

block content
    h1= title
    p Welcome to #{title}
    #hierarchy-wrapper
        - each relation in assignedStorages
            .relation-item(style="overflow: hidden;")
                .source-item(style="float: left;")
                    span.relation= relation.name + " -> "
                .target-item(style="float: left;")
                    span.relation= relation.targetStorage.name
